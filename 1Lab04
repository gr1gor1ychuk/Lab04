#include <stdio.h>
#include <math.h>

int main()
{
    int x, y, a, b;

    printf("Arifmetic Operators:\n");
    printf("Input X:\n");
    scanf( "%d", &x);
    printf("Input Y:\n");
    scanf( "%d", &y);

    printf("Sum: %d\n", x+y);
    printf("Substract: %d\n", x-y);
    printf("Multiply: %d\n", x*y);
    printf("Divide: %d\n", x/y);
    printf("Remainder: %d\n", x%y);
    printf("Increment of X: %d\n", ++x);
    --x;
    printf("Decrement of X: %d\n", --x);

    printf("\n");

    printf("Logic Operators:\n");

    printf("Input A:\n");
    scanf( "%d", &a);
    printf("Input B:\n");
    scanf( "%d", &b);

    printf("AND:");
    printf("%d\n", a>5 && b<10);
    printf("OR:");
    printf("%d\n", a>5 || b<10);
    printf("NOT:");
    printf("%d\n", !(a>5 || b<10));

    return 0;
}
